{
  "name": "@alexmc2/create-express-api-starter",
  "version": "0.1.5",
  "description": "Interactive CLI to scaffold API-first Express backend projects",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexmc2/create-express-api-starter.git"
  },
  "bugs": {
    "url": "https://github.com/alexmc2/create-express-api-starter/issues"
  },
  "homepage": "https://github.com/alexmc2/create-express-api-starter#readme",
  "keywords": [
    "express",
    "express-generator",
    "scaffold",
    "cli",
    "starter",
    "boilerplate",
    "api",
    "rest",
    "typescript",
    "javascript",
    "postgres",
    "jest",
    "supertest"
  ],
  "bin": {
    "create-express-api-starter": "dist/cli.js"
  },
  "files": [
    "dist",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "tsx src/cli/index.ts",
    "build": "tsup",
    "lint": "eslint . --ext .ts",
    "test": "vitest run",
    "test:unit": "vitest run --exclude test/integration.generated-projects.test.ts",
    "pack": "npm pack",
    "prepack": "npm run build",
    "prepublishOnly": "npm run test:unit"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.1",
    "ejs": "^4.0.1",
    "execa": "^9.6.1",
    "fs-extra": "^11.3.3",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.19.33",
    "eslint": "^8.57.1",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}

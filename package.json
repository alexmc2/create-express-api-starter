{
  "name": "@alexmc2/create-express-api-starter",
  "version": "0.1.0",
  "description": "Interactive CLI to scaffold API-first Express backend projects",
  "license": "MIT",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexmc2/create-express-api-starter.git"
  },
  "bugs": {
    "url": "https://github.com/alexmc2/create-express-api-starter/issues"
  },
  "homepage": "https://github.com/alexmc2/create-express-api-starter#readme",
  "keywords": [
    "express",
    "api",
    "scaffold",
    "starter",
    "cli",
    "generator",
    "typescript",
    "javascript"
  ],
  "bin": {
    "create-express-api-starter": "dist/cli.js"
  },
  "files": [
    "dist",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "dev": "tsx src/cli/index.ts",
    "build": "tsup",
    "test": "vitest run",
    "test:unit": "vitest run --exclude test/integration.generated-projects.test.ts",
    "pack": "npm pack",
    "prepack": "npm run build",
    "prepublishOnly": "npm run test:unit && npm run build"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@clack/prompts": "^0.10.1",
    "ejs": "^3.1.10",
    "execa": "^9.5.2",
    "fs-extra": "^11.2.0",
    "picocolors": "^1.1.1"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.17.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  }
}
